
// Firestore rules have been cleared as Firebase integration was removed.
// If you re-integrate Firebase, you will need to define appropriate
// security rules for your collections.
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // Default deny all reads and writes
    match /{document=**} {
      allow read, write: if false;
    }
  }
}
