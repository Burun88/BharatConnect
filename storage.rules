
rules_version = '2';

// WARNING: These rules are for debugging purposes ONLY and are not secure.
// They allow anyone to read and write to your entire storage bucket.
// We will revert to more secure rules once we have diagnosed the issue.
service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read, write;
    }
  }
}
