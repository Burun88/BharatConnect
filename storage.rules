
// Firebase Storage rules have been cleared as Firebase integration was removed.
// If you re-integrate Firebase Storage, you will need to define appropriate
// security rules for your buckets and files.
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Default deny all reads and writes
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}
